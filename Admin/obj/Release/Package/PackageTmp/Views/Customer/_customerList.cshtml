@model List<Services.ViewModels.CustomerViewModel>

@foreach (var item in Model)
{
    <tr>
        <td>@item.Email</td>
        <td>@item.FullName</td>
        <td>@item.PhoneNo</td>
        <td>@item.Address</td>
        <td><input type="checkbox" class="cbStatus" @(item.Status == true? "checked": "") data-id="@item.Id" /></td>
        <td><a href="/Customer/Edit?id=@item.Id">Sửa</a></td>
    </tr>
}
